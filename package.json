{
  "name": "msw-connect-web",
  "version": "0.2.0",
  "description": "Mock Service Worker utility for connect-web",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "require": {
      "types": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.d.ts"
    },
    "default": {
      "types": "./dist/esm/index.js",
      "default": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "prepack": "pnpm build",
    "test": "pnpm -r test",
    "lint": "rome ci .",
    "fmt": "rome format ."
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^1.0.0",
    "@changesets/cli": "^2.26.0",
    "msw": "^1.0.0",
    "rome": "^11.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "msw": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssssota/msw-connect-web.git"
  },
  "keywords": [
    "msw",
    "connect-web",
    "grpc",
    "mock"
  ],
  "author": "TOMIKAWA Sotaro <tommysouta@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ssssota/msw-connect-web/issues"
  },
  "homepage": "https://github.com/ssssota/msw-connect-web#readme",
  "packageManager": "pnpm@7.26.1"
}
